algoritmo "Positivo ou negativo "
// Função :
// Autor :
// Data : 06/05/2013
// Seção de Declarações 
var
	valor : real

inicio
	// Seção de Comandos
	escreval ( "Entre com o numero:")
	leia(valor)
	se valor > 0 entao
		escreval( "O numero é positivo!!")
	senao
		escreval("O numero é negativo!!")    
	fimse
fimalgoritmo
