algoritmo "Transformar m em cm"
// Função :
// Autor :
// Data : 06/05/2013
// Seção de Declarações 
var
	valorMetro : real
	valorCentimetro : real

inicio
	// Seção de Comandos 
	escreval("Entre com o valor em metros:")
	leia(valorMetro)
	valorCentimetro <- valorMetro * 100
	escreval("O valor em centimetros é:" ,valorCentimetro)
fimalgoritmo
