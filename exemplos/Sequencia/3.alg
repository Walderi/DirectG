algoritmo "Soma de 2 numeros"
// Função :
// Autor :
// Data : 06/05/2013
// Seção de Declarações 
var
	x : inteiro
	y : inteiro
	soma : inteiro

inicio
	// Seção de Comandos 
	escreval("Entre com o primeiro numero:")
	leia(x)
	escreval("Entre com o segundo numero:")
	leia(y)
	soma <- x + y
	escreval("A soma foi:" ,soma)
fimalgoritmo
